{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "TicTacToe": {
      "commandName": "Project",
      "commandLineArgs": "$(SolutionDir)/Examples/tic-tac-toe.ol TicTacToe"
    },
    "String and comments": {
      "commandName": "Project",
      "commandLineArgs": "$(SolutionDir)/Examples/strings.ol Main"
    },
    "Unterminated string": {
      "commandName": "Project",
      "commandLineArgs": "$(SolutionDir)/Examples/wrong_string.ol Main"
    },
    "Sum of two numbers": {
      "commandName": "Project",
      "commandLineArgs": "$(SolutionDir)/Examples/sum-of-two.ol Main 42 -84"
    },
    "Chain calls": {
      "commandName": "Project",
      "commandLineArgs": "$(SolutionDir)/Examples/chain-calls.ol Main 7"
    },
    "Compilation v2 mogus": {
      "commandName": "Project",
      "commandLineArgs": "$(SolutionDir)/Examples/chain-calls.ol $(SolutionDir)/Build/mogus/mogus.exe Main"
    },
    "Compilation v2 Tic-tac-toe": {
      "commandName": "Project",
      "commandLineArgs": "$(SolutionDir)/Examples/tic-tac-toe.ol $(SolutionDir)/Build/mogus/mogus.exe TicTacToe"
    },
    "Compilation v2 Gregification": {
      "commandName": "Project",
      "commandLineArgs": "$(SolutionDir)/Examples/inheritance.ol $(SolutionDir)/Build/mogus/gregification.exe Main"
    }
  }
}